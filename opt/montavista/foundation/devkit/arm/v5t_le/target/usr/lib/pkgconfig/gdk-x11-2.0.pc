prefix=/usr
exec_prefix=/usr
libdir=/usr/lib
includedir=/usr/include
target=x11

Name: GDK
Description: GIMP Drawing Kit (${target} target)
Version: 2.6.1
Requires: gdk-pixbuf-2.0 pangoxft pangox
Libs: -L${libdir} -lgdk-${target}-2.0  -lXrandr -lXrender -lXext  -lXfixes -lX11   -lXcursor -lXrender -lX11 -lXext   
Cflags: -I${includedir}/gtk-2.0 -I${libdir}/gtk-2.0/include -DXTHREADS -D_REENTRANT -DXUSE_MTSAFE_API -I/usr/X11R6/include   -DXTHREADS -D_REENTRANT -DXUSE_MTSAFE_API -I/usr/X11R6/include   
